#include <stdio.h>

int main(void)
{
  /*loop to print numbers from 1 to 100 but prints fizz for multiples of 3 and Buzz for multiples of 5, fizzbuzz for both*/ 
  int i;
  for (i=1;i<101;i++) 
    {
      if (i%3==0){ /*multiple of 3*/
        printf("Fizz");
      }
      else if (i%5==0){ /*multiple of 5*/
        printf("Buzz");
      }
      else if (i%3==0 && i%5==0){ /*multiple of 3and5*/
	printf("FizzBuzz");
      }
      else {
	printf("%d", i);
      }

      if (i!=100) /*space*/
        {
          printf(" ");
        }
    }
  return (0);
}
